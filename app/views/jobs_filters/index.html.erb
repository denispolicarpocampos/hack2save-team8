<section class="content-header">
  <h1>
    Jobs
  </h1>
</section>

<section class="content">
  <% @jobs.each do |job| %>
    <%= render layout: "jobs/job", locals: { job: job } do %>
      <%= link_to "Subscribe", new_user_subscription_path(job_id: job), class: "btn btn-warning btn-flat" %>
    <% end %>
  <% end %>
</section>
